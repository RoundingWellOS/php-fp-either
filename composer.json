{
    "authors": [
        {
            "name": "Tom",
            "email": "tomjharding@live.co.uk"
        },
        {
            "name": "Woody Gilk",
            "email": "woody.gilk@gmail.com",
            "role": "Contributor"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpFp\\Either\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpFp\\Either\\Test\\": "test/"
        }
    },
    "description": "An implementation of the Either monad in PHP.",
    "homepage": "http://www.github.com/php-fp/php-fp-either",
    "keywords": ["functional", "monad", "either"],
    "license": "MIT",
    "name": "php-fp/php-fp-either",
    "time": "2016-04-19",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "spatie/7to5": "^1.1"
    },
    "scripts": {
        "php5ize": [
            "php7to5 convert --overwrite --copy-all -- src/ src-php5/",
            "php7to5 convert --overwrite --copy-all -- test/ test-php5/",
            "rm -rf src && mv src-php5 src",
            "rm -rf test && mv test-php5 test"
        ],
        "test": "phpunit test/"
    }
}
